    plant def Melding(alg bool conditie):
      event e_bevestig, e_aan;
      location uit:
        initial;
        edge e_aan when conditie goto aan;
      location aan:
        edge e_bevestig goto bevestigd;
      location bevestigd:
        edge e_aan when conditie goto aan;
    end

    MeldingSOSRechterbuis: Melding(not Tunnel_Verkeersbuis1_SOS.uit);
    MeldingHulppostkastARechterbuis: Melding(Tunnel_Verkeersbuis1_HulppostkastA_SensorOpen.aan or Tunnel_Verkeersbuis1_HulppostkastA_Brandslang.aan or Tunnel_Verkeersbuis1_HulppostkastA_Handblusser.aan or Tunnel_Verkeersbuis1_HulppostkastA_Noodtelefoon.aan);
    MeldingHulppostkastCRechterbuis: Melding(Tunnel_Verkeersbuis1_HulppostkastC_SensorOpen.aan or Tunnel_Verkeersbuis1_HulppostkastC_Handblusser.aan or Tunnel_Verkeersbuis1_HulppostkastC_Noodtelefoon.aan);
    MeldingSOSLinkerbuis: Melding(not Tunnel_Verkeersbuis2_SOS.uit);
    MeldingHulppostkastALinkerbuis: Melding(Tunnel_Verkeersbuis2_HulppostkastA_SensorOpen.aan or Tunnel_Verkeersbuis2_HulppostkastA_Brandslang.aan or Tunnel_Verkeersbuis2_HulppostkastA_Handblusser.aan or Tunnel_Verkeersbuis2_HulppostkastA_Noodtelefoon.aan);
    MeldingHulppostkastCLinkerbuis: Melding(Tunnel_Verkeersbuis2_HulppostkastC_SensorOpen.aan or Tunnel_Verkeersbuis2_HulppostkastC_Handblusser.aan or Tunnel_Verkeersbuis2_HulppostkastC_Noodtelefoon.aan);
    MeldingNoodbediening: Melding(Tunnel_Noodbediening.aan);

    plant GeselecteerdeMelding:
      monitor Tunnel_Verkeersbuis1_MeldingSOS_Knop_u_indrukken, Tunnel_Verkeersbuis2_MeldingSOS_Knop_u_indrukken,
              Tunnel_Verkeersbuis1_MeldingHulppostkastA_Knop_u_indrukken, Tunnel_Verkeersbuis2_MeldingHulppostkastA_Knop_u_indrukken,
              Tunnel_Verkeersbuis1_MeldingHulppostkastC_Knop_u_indrukken, Tunnel_Verkeersbuis2_MeldingHulppostkastC_Knop_u_indrukken,
              Tunnel_MeldingNoodbediening_Knop_u_indrukken;
      location geen:
        initial;
        edge Tunnel_Verkeersbuis1_MeldingSOS_Knop_u_indrukken goto SOSRechts;
        edge Tunnel_Verkeersbuis1_MeldingHulppostkastA_Knop_u_indrukken goto HulppostkastARechts;
