plant def HybrideTimer(event aan,reset,timeout;alg real looptijd):
  cont t = 0 der if uit:       0
               elif afgelopen: 0
               else -1 end;
  location uit:
      initial; marked;
      edge aan do t := looptijd goto lopend;
  location lopend:
      edge reset goto uit;
      edge timeout when t <= 0 goto afgelopen;
  location afgelopen:
      edge reset goto uit;
end

group def copyAndRelocate(alg string defName, postName; alg int x, y):
    svgcopy id defName post postName;
    svgmove id defName+postName to x,y;
end
group def CopyAndRename(alg string defName, postName, textID, textValue):
    svgcopy id defName post postName;
    svgout id textID text value textValue;
end

