// Blok ventilatie rechts
 group besturing_blokken_overzicht_systemen_en_detecties_ventilatie_rb:
     svgin id "blokken_overzicht_systemen_en_detecties" + "_ventilatie"+rb event
         if rb_overzicht_systemen_en_detecties_ventilatie.dicht : rb_overzicht_systemen_en_detecties_ventilatie.e_Open else rb_overzicht_systemen_en_detecties_ventilatie.e_Sluit end;
     overzicht_systemen_en_detecties_ventilatie_rb:copyAndRelocate("overzicht_systemen_en_detecties_ventilatie", rb, 345, 370);
     svgout id "overzicht_systemen_en_detecties_ventilatie"+ rb attr "visibility"
        value if rb_overzicht_systemen_en_detecties_ventilatie.open and rb_overzicht_systemen_en_detecties_buis.open:"visible" else "hidden" end;
     svgout id "sub_blokken_overzicht_systemen_en_detecties_rect" + "_ventilatie"+rb attr "fill"
        value if rb_overzicht_systemen_en_detecties_ventilatie.open: "gold" else "slategrey" end;

     svgin id "Langsventilatie_handmatig_knop"+rb event Tunnel_Verkeersbuis1_BedieningVentilatie_KnopHand_Knop_u_indrukken;
     svgin id "Langsventilatie_auto_knop"+rb event Tunnel_Verkeersbuis1_BedieningVentilatie_KnopAuto_Knop_u_indrukken;

     svgout id "Langsventilatie_auto_knop"+rb attr "visibility"
        value if rb_overzicht_systemen_en_detecties_ventilatie_dropdown_percentage_stand.dicht and Tunnel_Verkeersbuis1_BedieningVentilatie_Modus.hand and rb_overzicht_systemen_en_detecties_ventilatie.open: "visible" else "hidden" end;
     svgout id "Langsventilatie_handmatig_knop"+rb attr "visibility"
        value if Tunnel_Verkeersbuis1_BedieningVentilatie_Modus.auto and rb_overzicht_systemen_en_detecties_ventilatie.open: "visible" else "hidden" end;
     svgout id "langsventilatie_handmatig"+rb attr "visibility"
        value if Tunnel_Verkeersbuis1_BedieningVentilatie_Modus.hand and rb_overzicht_systemen_en_detecties_ventilatie.open and not Tunnel_Verkeersbuis1_BedieningVentilatie_Modus.auto : "visible" else "hidden" end;
     svgout id "Langsventilatie_auto"+rb attr "visibility"
        value if Tunnel_Verkeersbuis1_BedieningVentilatie_Modus.auto and rb_overzicht_systemen_en_detecties_ventilatie.open and not Tunnel_Verkeersbuis1_BedieningVentilatie_Modus.hand : "visible" else "hidden" end;


     svgin id "overzicht_systemen_en_detecties_ventilatie_dropdown_standen_langsventilatie_knop"+rb event
        if rb_overzicht_systemen_en_detecties_ventilatie_dropdown_percentage_stand.dicht and Tunnel_Verkeersbuis1_BedieningVentilatie_Modus.hand: rb_overzicht_systemen_en_detecties_ventilatie_dropdown_percentage_stand.e_Open elif rb_overzicht_systemen_en_detecties_drop_down_richting_ventilatie.open: rb_overzicht_systemen_en_detecties_drop_down_richting_ventilatie.e_Sluit else rb_overzicht_systemen_en_detecties_ventilatie_dropdown_percentage_stand.e_Sluit end;
     rb_overzicht_systemen_en_detecties_ventilatie_dropdown_standen_langsventilatie:copyAndRelocate("overzicht_systemen_en_detecties_ventilatie_dropdown_standen_langsventilatie", rb, 528, 430);
    svgout id "overzicht_systemen_en_detecties_ventilatie_dropdown_standen_langsventilatie" +rb attr "visibility"
