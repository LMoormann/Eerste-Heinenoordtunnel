    group def GUI_Melding(alg string melding, camera_nummer, camera_tekst, melding_tekst; alg int y_onbevestigd, y_bevestigd; alg bool MeldingAan, MeldingGeselecteerd, MeldingBevestigd, MeldingOnderdrukt; uncontrollable u_indrukken):
      Onbevestigd: copyAndRelocate("blok_meldingen", "_"+melding+"_onbevestigd", 730, y_onbevestigd);
      svgout id "blok_melding_bijbehorende_camera_"+melding+"_onbevestigd" text value camera_nummer;
      svgout id "blok_meldingen_bijbehorende_locatie_"+melding+"_onbevestigd" text value camera_tekst;
      svgout id "blok_meldingen_naam_melding_"+melding+"_onbevestigd" text value melding_tekst;
      svgout id "blok_meldingen_"+melding+"_onbevestigd" attr "visibility"
          value if MeldingAan and not MeldingOnderdrukt: "visible" else "hidden" end;
      svgout id "blok_meldingen_achtergrond_"+melding+"_onbevestigd" attr "stroke"
          value if MeldingGeselecteerd and not MeldingOnderdrukt: "yellow" else "darkslategrey" end;
      svgin id "blok_meldingen_"+melding+"_onbevestigd" event u_indrukken;
      Bevestigd: copyAndRelocate("blok_meldingen", "_"+melding+"_bevestigd", 730, y_bevestigd);
      svgout id "blok_melding_bijbehorende_camera_"+melding+"_bevestigd" text value camera_nummer;
      svgout id "blok_meldingen_bijbehorende_locatie_"+melding+"_bevestigd" text value camera_tekst;
      svgout id "blok_meldingen_naam_melding_"+melding+"_bevestigd" text value melding_tekst;
      svgout id "blok_meldingen_bijbehorende_alarm_deelsysteemalarm_"+melding+"_bevestigd" attr "visibility" value "hidden";
      svgout id "blok_meldingen_bijbehorende_alarm_handmatigalarm_"+melding+"_bevestigd" attr "visibility" value "hidden";
      svgout id "blok_meldingen_"+melding+"_bevestigd" attr "visibility"
          value if MeldingBevestigd and not MeldingOnderdrukt and openen_en_sluiten_bevestigde_meldingen.open: "visible" else "hidden" end;
    end

